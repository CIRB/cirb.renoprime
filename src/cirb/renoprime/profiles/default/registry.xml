<?xml version="1.0"?>
<registry>
    <record name="cirb.renoprime.gis_url" remove="true">
    </record>
    <record name="cirb.renoprime.my_brugis_url" remove="true">
    </record>
    <record name="cirb.renoprime.gis_basemap_url_prefix">
        <field type="plone.registry.field.URI">
            <title>GIS basemap URL prefix</title>
            <description>To fetch GIS basemaps</description>
        </field>
        <value>https://geoservices-urbis.irisnet.be</value>
    </record>
    <record name="cirb.renoprime.gis_localization_url_prefix">
        <field type="plone.registry.field.URI">
            <title>GIS localization URL prefix</title>
            <description>To fetch localization</description>
        </field>
        <value>https://geoservices.irisnet.be/localization</value>
    </record>
</registry>

